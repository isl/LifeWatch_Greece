<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>eu.lifewatch</groupId>
    <artifactId>DataServices-middleware-client</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>jar</packaging>
    <build>
        <resources>
            <resource>
                <targetPath>META-INF</targetPath>
                <directory>src</directory>
                <includes>
                    <include>jax-ws-catalog.xml</include>
                    <include>wsdl/**</include>
                </includes>
            </resource>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.jvnet.jax-ws-commons</groupId>
                <artifactId>jaxws-maven-plugin</artifactId>
                <version>2.3</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <wsdlFiles>
                                <wsdlFile>localhost_8080/DataServices-middleware/DirectoryService_Search.wsdl</wsdlFile>
                            </wsdlFiles>
                            <packageName>eu.lifewatch.dataservices.middleware.client.directory</packageName>
                            <wsdlLocation>http://localhost:8080/DataServices-middleware/DirectoryService_Search?wsdl</wsdlLocation>
                            <staleFile>${project.build.directory}/jaxws/stale/DirectoryService_Search.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-DirectoryService_Search</id>
                        <phase>generate-sources</phase>
                    </execution>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <wsdlFiles>
                                <wsdlFile>localhost_8080/DataServices-middleware/DirectoryService_Insert.wsdl</wsdlFile>
                            </wsdlFiles>
                            <packageName>eu.lifewatch.dataservices.middleware.client.directory</packageName>
                            <wsdlLocation>http://localhost:8080/DataServices-middleware/DirectoryService_Insert?wsdl</wsdlLocation>
                            <staleFile>${project.build.directory}/jaxws/stale/DirectoryService_Insert.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-DirectoryService_Insert</id>
                        <phase>generate-sources</phase>
                    </execution>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <wsdlFiles>
                                <wsdlFile>localhost_8080/DataServices-middleware/DirectoryService_Update.wsdl</wsdlFile>
                            </wsdlFiles>
                            <packageName>eu.lifewatch.dataservices.middleware.client.directory</packageName>
                            <wsdlLocation>http://localhost:8080/DataServices-middleware/DirectoryService_Update?wsdl</wsdlLocation>
                            <staleFile>${project.build.directory}/jaxws/stale/DirectoryService_Update.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-DirectoryService_Update</id>
                        <phase>generate-sources</phase>
                    </execution>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <wsdlFiles>
                                <wsdlFile>localhost_8080/DataServices-middleware/DirectoryService_Delete.wsdl</wsdlFile>
                            </wsdlFiles>
                            <packageName>eu.lifewatch.dataservices.middleware.client.directory</packageName>
                            <wsdlLocation>http://localhost:8080/DataServices-middleware/DirectoryService_Delete?wsdl</wsdlLocation>
                            <staleFile>${project.build.directory}/jaxws/stale/DirectoryService_Delete.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-DirectoryService_Delete</id>
                        <phase>generate-sources</phase>
                    </execution>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <wsdlFiles>
                                <wsdlFile>localhost_8080/DataServices-middleware/MetadataRepository_Insert.wsdl</wsdlFile>
                            </wsdlFiles>
                            <packageName>eu.lifewatch.dataservices.middleware.client.metadata</packageName>
                            <wsdlLocation>http://localhost:8080/DataServices-middleware/MetadataRepository_Insert?wsdl</wsdlLocation>
                            <staleFile>${project.build.directory}/jaxws/stale/MetadataRepository_Insert.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-MetadataRepository_Insert</id>
                        <phase>generate-sources</phase>
                    </execution>
                </executions>
                <dependencies>
                    <dependency>
                        <groupId>javax.xml</groupId>
                        <artifactId>webservices-api</artifactId>
                        <version>2.0</version>
                    </dependency>
                </dependencies>
                <configuration>
                    <sourceDestDir>${project.build.directory}/generated-sources/jaxws-wsimport</sourceDestDir>
                    <xnocompile>true</xnocompile>
                    <verbose>true</verbose>
                    <extension>true</extension>
                    <catalog>${basedir}/src/jax-ws-catalog.xml</catalog>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.17</version>
        </dependency>
    </dependencies>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>1.7</maven.compiler.source>
        <maven.compiler.target>1.7</maven.compiler.target>
    </properties>
</project>